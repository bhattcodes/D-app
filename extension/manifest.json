{
    "manifest_version": 2,
    "name": "abx extension",
    "description": "extensions _abx",
    "version": "1.0",
    "content_scripts": [
        {
            "js": [
                "contentscript.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions" : ["system.cpu",
        "system.storage",
        "system.memory",
        "system.display"
    ],
    "browser_action": {
        "default_popup": "index.html"
    }
}